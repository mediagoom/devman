{
  "name": "@mediagoom/devman",
  "version": "0.0.7",
  "description": "A nodejs developer manager",
  "bin": {
    "devman": "./index.js"
  },
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "interactive": "node index.js",
    "integration": "node test/index.js",
    "start": "node index.js start",
    "stop": "node index.js stop",
    "testinstrumented" : "nyc --show-process-tree npm test ",
    "report" : "nyc report",
    "test": "run-s -c start integration stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mediagoom/devman.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "development"
  ],
  "author": "MediaGoom",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mediagoom/devman/issues"
  },
  "homepage": "https://github.com/mediagoom/devman#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "nyc": "^13.0.1",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "chokidar": "^1.6.1",
    "debug": "^3.1.0",
    "express": "^4.14.1",
    "yargs": "^12.0.1"
  }
}
