{
    "proc" : [
        {
              "name"  : "console1"
            , "watch" : ["test/**/*js"]
            , "cmd"   : {"proc": "node", "args": ["./test/consoleapp.js"]}
            , "debug" : false 
            , "break" : false
            , "options" : { "env":{
                                     "DEVMANID" : "PROCESS-1"
                                     , "CONSOLECHAR" : "[1]"
                                  }
                          }
                          
         }
         , 
         {
              "name"  : "console2"
            , "watch" : ["test/**/*js"]
            , "cmd"   : {"proc": "node", "args": ["./test/consoleapp.js"]}
            , "debug" : false 
            , "break" : false
            , "options" : { "env":{
                                      "DEVMANID" : "PROCESS-2"
                                    , "CONSOLECHAR" : "[2]"
                                  }
                          }
                          
         }
         , 
         {
              "name"  : "just say hello"
            , "watch" : ["test/**/*js"]
            , "exec"  : ["node ./test/consoleapp.js once -m \"hello world\""]
            , "debug" : false 
            , "break" : false
            , "options" : { "env":{
                                      "DEVMANID" : "PROCESS-3"
                                    , "CONSOLECHAR" : "[3]"
                                  }
                          }
                          
         }
         , 
         {
              "name"  : "npm test"
            , "watch" : ["test/**/*js"]
            , "exec"  : ["node ./test/consoleapp.js once -m \"[4] npm-test\""]
            , "cmd"   : {"proc": "npm", "args": ["--version"]}
            , "debug" : false 
            , "break" : false
            , "options" : {     "shell" : true
                                , "env":{
                                      "DEVMANID" : "PROCESS-4"
                                    , "CONSOLECHAR" : "[4]"
                                    
                                  }
                          }
                          
         }
    ]

}
